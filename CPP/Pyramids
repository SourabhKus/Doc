#include <stdio.h>
#include <iostream>

using namespace std;

void printInsideValues(int count, int i)
{
    for(int j = 1; j <= (count - i); j++)
         cout<<" ";
        
    for(int k = 1; k <= i; k++)
        cout<<k<<" ";
            
    cout<<endl;
}

int main()
{
    int count = 4;
    
    for(int i = 1;  i <= count; i++)
        printInsideValues(count, i);
    
    for(int i = (count - 1);  i > 0; i--)
        printInsideValues(count, i);
    
    return 0;
}
